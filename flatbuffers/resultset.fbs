namespace BlazingDB.Protocol;

enum ColumnType : byte {
  Bool = 0,
  Char,
  Short,
  Int,
  Long,
  Float,
  Double,
  String,
  Date
}

table Column {
  column_name:string;
  column_type:ColumnType;
}

// Number of values is the same of number of columns in the row
table Row {
  row_values:[string];
}

table ResultSet {
  columns:[Column];
  rows:[Row];
}
